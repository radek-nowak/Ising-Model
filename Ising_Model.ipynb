    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "N = 10 # Size of NxN lattice\n",
    "\n",
    "\n",
    "def initial_lattice(N):\n",
    "    '''\n",
    "    Returns matrix of size NxN. The elements are either +1 or -1. The sign corresponds \n",
    "    to the value of a spin.\n",
    "    \n",
    "    Args:\n",
    "        N: int - size of a lattice\n",
    "    Returns:\n",
    "        lattice: array(int, size = [N,N])\n",
    "    '''\n",
    "    lattice = 2*np.random.randint(2,size=[N,N])-1\n",
    "    return lattice\n",
    "\n",
    "a = initial_lattice(N)\n",
    "a"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
